{
  "version": 2,
  "name": "cvpilot",
  "builds": [
    {
      "src": "app/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://cvpilot-670621051738.asia-northeast3.run.app"
  }
} 